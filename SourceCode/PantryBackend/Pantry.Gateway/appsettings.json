{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "goods-route": {
        "ClusterId": "goods-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/goods/{**catch-all}"
        }
      },
      "goodratings-route": {
        "ClusterId": "goodratings-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/goodratings/{**catch-all}"
        }
      },
      "suggestions-route": {
        "ClusterId": "suggestions-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/suggestions/{**catch-all}"
        }
      },
      "meals-route": {
        "ClusterId": "meals-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/meals/{**catch-all}"
        }
      },
      "recipes-route": {
        "ClusterId": "recipes-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/recipes/{**catch-all}"
        }
      },
      "ingredients-route": {
        "ClusterId": "ingredients-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/ingredients/{**catch-all}"
        }
      },
      "recipedetails-route": {
        "ClusterId": "recipedetails-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/recipedetails/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "goods-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://pantry.api:8080/"
          }
        }
      },
      "goodratings-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://pantry.api:8080/"
          }
        }
      },
      "suggestions-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://pantry.api:8080/"
          }
        }
      },
      "meals-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://pantry.plan.api:8080/"
          }
        }
      },
      "recipes-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://pantry.recipe.api:8080/"
          }
        }
      },
      "ingredients-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://pantry.recipe.api:8080/"
          }
        }
      },
      "recipedetails-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://pantry.recipe.api:8080/"
          }
        }
      }
    }
  }
}
